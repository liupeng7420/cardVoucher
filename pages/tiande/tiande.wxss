/* pages/xishiduo/xishiduo.wxss */

.big {
  width: 92%;
  margin: 0 auto;
}

.big>image {
  width: 100%;
  height: 450rpx;
  margin-bottom: 50rpx;
}

.lpk>view:nth-of-type(1)>image {
  width: 106rpx;
  height: 51rpx;
}

.lpk>view:nth-of-type(1) {
  display: flex;
  align-items: center;
  width: 92%;
  margin: 0 auto;
  margin-bottom: 30rpx;
}

.xjq {
  width: 92%;
  margin: 0 auto;
}

.xjq>image {
  width: 48%;
  height: 300rpx;
  margin-bottom: 30rpx;
  box-shadow:0 1rpx 30rpx 0  #000003;
  border-radius: 10rpx;
}
.xjq>image:nth-of-type(1),.xjq:nth-of-type(3){
  margin-right: 4%;
}
